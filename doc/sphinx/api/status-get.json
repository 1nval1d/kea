{
    "avail": "1.7.3",
    "brief": [
        "This command returns server runtime information.",
        "It takes no arguments."
    ],
    "cmd-syntax": [
        "{",
        "    \"command\": \"status-get\"",
        "}"
    ],
    "description": "See <xref linkend=\"command-status-get\"/>",
    "name": "status-get",
    "resp-comment": [
        "If the HA hook is loaded it adds HA servers info to the response."
    ],
    "resp-syntax": [
        "{",
        "    \"result\": 0,",
        "    \"arguments\": {",
        "        \"pid\": 1234,",
        "        \"uptime\": 59,",
        "        \"reload\": 10,",
        "        ...",
        "    }",
        "}"
    ],

    "support": [
        "kea-dhcp4",
        "kea-dhcp6",
        "kea-dhcp-ddns",
        "kea-ctrl-agent"
    ]
}
